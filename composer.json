{
    "name": "hieofone-directory/hieofone-directory",
    "description": "Health Information Exchange of One Directory Server.",
    "keywords": [
        "hieofone",
        "laravel",
        "uma",
        "oauth2",
        "oidc",
        "php",
        "javascript",
        "health",
        "fhir",
        "medical"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Michael Shihjay Chen",
            "email": "shihjay2@gmail.com",
            "homepage": "http://noshemr.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "bshaffer/oauth2-server-httpfoundation-bridge": "^1.3",
        "bshaffer/oauth2-server-php": "^1.10",
        "doctrine/dbal": "^2.5",
        "google/apiclient": "~2.0",
        "graham-campbell/markdown": "10.*",
        "jenssegers/date": "*",
        "knplabs/github-api": "~1.7",
        "kordy/ticketit": ">=0.4.",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "~3.0",
        "laravelcollective/html": "^5.4.0",
        "naviocean/laravel-nameparser": "dev-master",
        "phpseclib/phpseclib": "~2.0",
        "riari/laravel-forum": "~4.0",
        "riari/laravel-forum-frontend": "~1.0",
        "shihjay2/openid-connect-uma-php": "dev-master",
        "simplesoftwareio/simple-qrcode": "~1.4",
        "socialiteproviders/generators": "^4.1",
        "socialiteproviders/manager": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "2.8.*|3.1.*",
        "symfony/dom-crawler": "2.8.*|3.1.*",
        "nunomaduro/collision": "^2.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "SocialiteProviders\\Doximity\\": "SocialiteProviders/Doximity/src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}